{
    "extends": "solium:recommended",
    "plugins": [
      "security"
    ],
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "indentation": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "blank-lines": "error",
      "no-trailing-whitespace": "error",
      "arg-overflow": [
        "error",
        4
      ]
    }
  }